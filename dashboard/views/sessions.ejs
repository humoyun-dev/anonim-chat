<!DOCTYPE html>
<html lang="uz">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Sessions List</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark mb-4">
    <div class="container-fluid">
      <a class="navbar-brand" href="/dashboard">Dashboard</a>
      <div class="d-flex">
        <a class="btn btn-outline-light me-2" href="/users">Users</a>
        <a class="btn btn-outline-light me-2" href="/messages">Messages</a>
        <a class="btn btn-outline-light" href="/logout">Logout</a>
      </div>
    </div>
  </nav>
  <div class="container">
    <h1 class="mb-4">Sessions List</h1>
    <table class="table table-bordered table-striped">
      <thead class="table-dark">
        <tr>
          <th>Anon User ID</th>
          <th>Owner ID</th>
        </tr>
      </thead>
      <tbody>
        <% sessions.forEach(session => { %>
          <tr>
            <td><%= session.anonUserId %></td>
            <td><%= session.ownerId %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
    <a href="/dashboard" class="btn btn-secondary">Back to Dashboard</a>
  </div>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
